{
  "presets": [
    [
      "@babel/preset-env",
//      {
//        "useBuiltIns": "entry" // entry usage false
//      }
      {
        "targets": {
          "node": "current",
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1",
          "ie": "11"
        },
        "useBuiltIns": false
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    ["@babel/plugin-transform-async-to-generator"]
  ]
}
