    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.4",
    "extract-loader": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "happy-mobile": "^0.0.21",
